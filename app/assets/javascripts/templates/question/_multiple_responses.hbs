<table class="answers-table">
  <thead>
    <tr>
      <th class='title'>Option</th>
      <th>Responses</th>
    </tr>
  </thead>
  <tbody>
    {{#each answers}}
    <tr>
      <td>{{@key}}</td>
      <td class='answers'>
        {{length}}
        {{> question/_response_tooltip answers=this}}
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>
